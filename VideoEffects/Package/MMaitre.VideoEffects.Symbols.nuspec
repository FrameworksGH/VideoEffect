  <metadata>
    <id>MMaitre.VideoEffects.Symbols</id>
    <title>Video Effects symbols</title>
    <version>$NuGetVersion$</version>
    <authors>Matthieu Maitre</authors>
    <owners>Matthieu Maitre</owners>
